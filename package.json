{
  "name": "vscode-search-everywhere",
  "displayName": "Search everywhere",
  "description": "Alternative for Go to symbol in workspace. Fast and fully customizable files, symbols search engine",
  "version": "0.0.1",
  "preview": true,
  "publisher": "kbysiec",
  "author": {
    "name": "Kamil Bysiec",
    "email": "kamil.bysiec@agileplayers.com",
    "url": "https://github.com/kbysiec"
  },
  "engines": {
    "vscode": "^1.41.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "Search",
    "Go to",
    "Browser"
  ],
  "activationEvents": [
    "onCommand:extension.helloWorld"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.41.0",
    "glob": "^7.1.5",
    "mocha": "^6.2.2",
    "typescript": "^3.6.4",
    "tslint": "^5.20.0",
    "vscode-test": "^1.2.2"
  },
